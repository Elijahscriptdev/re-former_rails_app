<%= form_for @user do |form| %>
    <p><%= form.label :username, "Enter your username" %></p>
      <%= form.text_field :username %>
    <p><%= form.label :email, "Enter your email" %></p>
      <%= form.email_field :email %>
    <p><%= form.label :password, "Enter your password" %></p>
      <%= form.password_field :password %>
      <p><%= form.submit %></p>
  <% end %>